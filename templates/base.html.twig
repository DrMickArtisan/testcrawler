<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        {# read and display all flash messages #}
                {% for label, messages in app.flashes %}
                    {% for message in messages %}
                        <div class="flash-{{ label }}">
                            <p>Flash message</p>
                            {{ message }}
                            <p>end flash message</p>
                        </div>
                    {% endfor %}
                {% endfor %}
        {{ form_start(form, {attr: {novalidate: 'novalidate',class:'webform-client-form webform-client-form-425 custom-form webform-conditional-processed'}}) }}
            {% block formulaire %}{% endblock %}
            <div class="form_actions">
                {% if form.Precedent is defined %}
                    {{form_widget(form.Precedent)}}
                {% endif %}
                {% if form.Suivant is defined %}
                    {{form_widget(form.Suivant)}}
                {% endif %}
            </div>
        {{ form_end(form) }}
    </body>
</html>
